{
  "name": "@cbuk100011/claude-mode",
  "version": "1.2.0",
  "description": "CLI launcher for Claude Code with multiple AI providers",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "claude-mode": "./dist/index.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --clean",
    "dev": "tsup src/index.ts --format esm --dts --watch",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@inquirer/prompts": "^7.2.1",
    "chalk": "^5.4.1",
    "commander": "^13.1.0",
    "dotenv": "^16.4.7"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "tsup": "^8.5.1",
    "typescript": "^5.8.3",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "claude",
    "claude-code",
    "openrouter",
    "ollama",
    "ai",
    "cli"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Cam10001110101/claude-mode-cli.git"
  },
  "homepage": "https://github.com/Cam10001110101/claude-mode-cli#readme",
  "bugs": {
    "url": "https://github.com/Cam10001110101/claude-mode-cli/issues"
  }
}
